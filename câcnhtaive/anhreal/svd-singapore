{
var _t = jQuery('title').html();
var _u = location.href;
_u = _u.replace('#ref-' + _ref_,'');
history.pushState(null, _t, _u);
}
}
var refSplit = window.location.hash.split('#');
var ref2='';
for (var x = 0; x < refSplit.length; x++) {
if (refSplit[x].indexOf("ref-")==0)
{
ref2 = refSplit[x].replace('ref-', '');
if(ref2 != '') {
break;
}
}
