function overrideRef(_ref_)
{
try{
delete window.document.referrer;
window.document.__defineGetter__('referrer', function () {return _ref_;});
} catch(e) {
try{
//delete window.document.referrer;
Object.defineProperty(document, "referrer", {get : function(){ return _ref_; }});
} catch(ex) {}
}
